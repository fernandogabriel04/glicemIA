# Configurações do Banco de Dados PostgreSQL (NeonDB)
DB_URL=jdbc:postgresql://seu-host.neon.tech/seu-banco?sslmode=require
DB_USERNAME=seu_usuario
DB_PASSWORD=sua_senha
DB_DRIVER=org.postgresql.Driver

# Pool de Conexões
DB_POOL_MIN_SIZE=5
DB_POOL_MAX_SIZE=20

# Configurações da API de IA
IA_PROVIDER=openai
IA_API_KEY=sua_api_key_aqui
IA_MODEL=gpt-4
IA_TIMEOUT_SECONDS=30
IA_FALLBACK_ENABLED=true
